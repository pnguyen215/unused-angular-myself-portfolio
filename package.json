  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --port 4204",
    "start:1024": "set NODE_OPTIONS=--max_old_space_size=1024 & ng serve --open --port 4204",
    "start:2148": "set NODE_OPTIONS=--max_old_space_size=2148 & ng serve --open --port 4204",
    "start:8192": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve --open --port 4204",
